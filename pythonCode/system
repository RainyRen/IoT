
config system
	option timezone 'UTC+8'
	option firmware_version 'v0.9.3'
	option loader_version 'v0.8.2'
	option linkit_firstboot '1'
	option hostname 'rainyren'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	list server '1.cn.pool.ntp.org'
	list server '1.asia.pool.ntp.org'
	list server '0.asia.pool.ntp.org'
	option enabled '1'
	option enable_server '0'

